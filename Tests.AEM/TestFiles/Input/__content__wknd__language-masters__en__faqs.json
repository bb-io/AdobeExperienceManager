            "title": {
              "jcr:title": "Need more help?",
              "cq:responsive": {
                "phone": {
                }
              }
            },
            "text": {
              "text": "<p style=\"text-align: left;\">Give us a call at <a href=\"#\">1-800-800-0000</a>.<br>\r\nE-mail us at info<a href=\"#\">@wknd.com</a><br>\r\nWe love to talk adventures!</p>\r\n",
              "cq:responsive": {
                "phone": {
                }
              }
            }
          }
        }
